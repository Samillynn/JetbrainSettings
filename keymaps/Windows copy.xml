    <keyboard-shortcut first-keystroke="alt 3" />
  </action>
  <action id="EditorDecreaseFontSize">
    <keyboard-shortcut first-keystroke="ctrl f11" />
  </action>
  <action id="EditorIncreaseFontSize">
    <keyboard-shortcut first-keystroke="ctrl f12" />
  </action>
</keymap>