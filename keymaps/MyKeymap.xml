<keymap version="1" name="MyKeymap" parent="Default for KDE">
  <action id="ActivateTerminalToolWindow">
    <keyboard-shortcut first-keystroke="alt f12" />
    <keyboard-shortcut first-keystroke="alt minus" />
  </action>
  <action id="ChooseRunConfiguration">
    <keyboard-shortcut first-keystroke="shift alt f10" />
    <keyboard-shortcut first-keystroke="shift f5" />
  </action>
  <action id="EditorDecreaseFontSize">
    <mouse-shortcut keystroke="control button142" />
  </action>
  <action id="EditorIncreaseFontSize">
    <mouse-shortcut keystroke="control button143" />
  </action>
  <action id="Graph.ApplyCurrentLayout" />
</keymap>